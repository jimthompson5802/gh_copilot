        "source=${localEnv:HOME}/Desktop,target=/openai,type=bind,consistency=cached"
	],
	"customizations": {
		"vscode": {
			"extensions": [
                "ms-python.python",
                "GitHub.copilot",
                "ms-azuretools.vscode-docker",
